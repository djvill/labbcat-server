          "options": {
            "tsConfig": [
              "projects/labbcat-common/tsconfig.lib.json",
              "projects/labbcat-common/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
